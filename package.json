    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-replace": "^6.0.1",
    "alive-server": "^1.2.9",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.0.2"
  }
